          <div id="pins" class="transitions-enabled">
            <% @pins.each do |pin| %>
              <div class="box_pin panel panel-default">
                <%= link_to image_tag(pin.image.url(:medium)), pin %>
                <div class="panel-body">
                  <%= pin.description %>
                  <br>
                  <p><%= raw pin.tag_list.map { |t| link_to t, tag_path(t) }.join(',  ') %></p>
                  <h5>posted by: <strong><%= pin.user.name if pin.user %></strong></h5>
                  <br/>
                  <% if pin.user == current_user %>
                    <div class="actions">
                      <%= link_to edit_pin_path(pin) do %>
                        Edit
                      <% end %>
                      <br>  
                      <%= link_to pin, method: :delete, data: { confirm: 'Are you sure?' } do %>
                        Delete
                      <% end %>
                    </div>
                  <% end %>
                </div>
              </div>
            <% end %>
